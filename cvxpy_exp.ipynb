   "source": [
    "import cvxpy as cp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "b6ac6bb0",
   "metadata": {},
   "outputs": [],
   "source": [
    "x = cp.Variable()\n",
    "y = cp.Variable()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "2a4265e3",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "optimal\n",
      "1.0\n",
      "1.0 1.570086213240983e-22\n"
     ]
    }
   ],
   "source": [
    "constraints = [x+y==1, \n",
    "              x-y>=1]\n",
    "\n",
    "obj = cp.Minimize((x-y)**2)\n",
    "\n",
    "prob = cp.Problem(obj, constraints)\n",
    "prob.solve()\n",
    "\n",
    "print(prob.status)\n",
    "print(prob.value)\n",
    "print(x.value, y.value)"
   ]
  },
